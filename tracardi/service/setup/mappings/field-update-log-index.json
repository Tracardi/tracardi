{
  "index_patterns": [
    "%%TEMPLATE_PATTERN%%"
  ],
  "template": {
    "settings": {
      "number_of_shards": %%CONF_SHARDS%%,
      "number_of_replicas": %%REPLICAS%%
    },
    "mappings": {
      "_meta": {
        "version": "%%VERSION%%",
        "name": "%%PREFIX%%"
      },
      "dynamic": "strict",
      "properties": {
        "id": {
          "type": "keyword",
          "ignore_above": 128
        },
        "timestamp": {
          "type": "date",
          "format": "yyyy-MM-dd HH:mm:ss.SSSSSS+00:00 || yyyy-MM-dd HH:mm:ss.SSSSSS || yyyy-MM-dd HH:mm:ss || strict_date_optional_time || epoch_millis"
        },
        "type": {
          "type": "keyword",
          "ignore_above": 32
        },
        "event_id": {
          "type": "keyword",
          "ignore_above": 40
        },
        "profile_id": {
          "type": "keyword",
          "ignore_above": 40
        },
        "session_id": {
          "type": "keyword",
          "ignore_above": 40
        },
        "source_id": {
          "type": "keyword",
          "ignore_above": 40
        },
        "field": {
          "type": "keyword",
          "ignore_above": 128
        },
        "value": {
          "type": "object",
          "enabled": false
        },
        "old_value": {
          "type": "object",
          "enabled": false
        }
      }
    },
    "aliases": {
      "%%ALIAS%%": {}
    }
  }
}