{
  "settings": {
    "number_of_shards": %%SHARDS%%,
    "number_of_replicas": %%REPLICAS%%
  },
  "mappings": {
    "_meta": {
      "version": "%%VERSION%%",
      "name": "%%PREFIX%%"
    },
    "dynamic": true,
    "properties": {
      "id": {
        "type": "keyword"
      },
      "metadata": {
        "dynamic": false,
        "properties": {
          "time": {
            "properties": {
              "insert": {
                "type": "date"
              }
            }
          }
        }
      }
    }
  },
  "aliases": {
    "%%ALIAS%%": {}
  }
}