{
  "index_patterns": [
    "%%TEMPLATE_PATTERN%%"
  ],
  "template": {
    "settings": {
      "number_of_shards": %%SHARDS%%,
      "number_of_replicas": %%REPLICAS%%,
      "index.mapping.total_fields.limit": 2000,
      "index.refresh_interval": "1s"
    },
    "mappings": {
      "_meta": {
        "version": "%%VERSION%%",
        "name": "%%PREFIX%%"
      },
      "dynamic": "strict",
      "properties": {
        "id": {
          "type": "keyword", "ignore_above": 64
        },
        "profile": {
          "properties": {
              "id": {
                "type": "keyword"
              }
          }
        },
        "metadata": {
          "properties":{
            "time": {
              "properties": {
                "insert": {
                  "type": "date",
                  "format": "yyyy-MM-dd HH:mm:ss||strict_date_optional_time ||epoch_millis"
                },
                "create": {
                  "type": "date",
                  "format": "yyyy-MM-dd HH:mm:ss||strict_date_optional_time ||epoch_millis"
                },
                "update": {
                  "type": "date",
                  "format": "yyyy-MM-dd HH:mm:ss||strict_date_optional_time ||epoch_millis"
                },
                "due": {
                  "type": "date",
                  "format": "yyyy-MM-dd HH:mm:ss||strict_date_optional_time ||epoch_millis"
                },
                "expire": {
                  "type": "date",
                  "format": "yyyy-MM-dd HH:mm:ss||strict_date_optional_time ||epoch_millis"
                }
              }
            }
          }
        },
        "type": {
          "type": "keyword"
        },
        "properties": {
          "type": "flattened"
        },
        "traits": {
          "type": "object",
          "dynamic": "true"
        }
      }
    },
    "aliases": {
      "%%ALIAS%%": {}
    }
  }
}