{
  "settings": {
    "number_of_shards": %%SHARDS%%,
    "number_of_replicas": %%REPLICAS%%
  },
  "mappings": {
    "_meta": {
      "version": "%%VERSION%%",
      "name": "%%PREFIX%%"
    },
    "dynamic": false,
    "properties": {
      "event_id": {
        "type": "keyword"
      },
      "profile_id": {
        "type": "keyword"
      },
      "flow_id": {
        "type": "keyword"
      },
      "origin": {
        "type": "keyword"
      },
      "metadata": {
        "properties": {
          "timestamp": {
            "type": "date"
          }
        }
      },
      "class_name": {
        "type": "keyword"
      },
      "module": {
        "type": "text"
      },
      "type": {
        "type": "keyword"
      },
      "message": {
        "type": "text"
      },
      "traceback": {
        "type": "keyword",
        "index": false
      }
    }
  },
  "aliases": {
    "%%ALIAS%%": {}
  }
}